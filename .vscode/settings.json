{
    "Laravel.basePath": "./laravel",
    "eslint.useFlatConfig": true,
    "eslint.workingDirectories": [
        "./laravel/"
    ],
    "eslint.options": {
        "overrideConfigFile": "./eslint.config.js"
    },
    "prettier.configPath": "./laravel/.prettierrc",
    "prettier.requireConfig": true,
    "[php]": {
        "editor.defaultFormatter": "open-southeners.laravel-pint"
    },
    "php.suggest.basic": false,
    "files.associations": {
        "*.module": "php"
    },
    "github.copilot.chat.codeGeneration.useInstructionFiles": true,
    // "i18n-ally.dirStructure": "dir",
    // "i18n-ally.localesPaths": [
    //     "laravel/lang"
    // ],
    // "i18n-ally.parsers.typescript.tsNodePath": "./laravel/node_modules/ts-node/dist/bin.js",
    // "i18n-ally.keystyle": "nested",
    // "i18n-ally.sourceLanguage": "en",
    // "i18n-ally.displayLanguage": "en",
    // "i18n-ally.enabledFrameworks": ["general"],
    // "i18n-ally.extract.keygenStyle": "nested",
    // "i18n-ally.namespace": true,
    "i18n-ally.pathMatcher": "php_{locale}.{ext}",
    "i18n-ally.localesPaths": [
        "laravel/lang"
    ],
    "i18n-ally.keystyle": "nested",
    // "i18n-ally.regex.key": "[\\w-.]+",
    // "i18n-ally.extract.autoDetect": true,
    // "i18n-ally.localeCountryMap": {
    //     "en": "php_en",
    //     "id": "php_id",
    //     "de": "php_de"
    // }
    "Laravel.translation.completion": false,
    "Laravel.translation.link": false,
    "laravel-pint.configPath": "./laravel/pint.json",
    "laravel-pint.enableDebugLogs": true,
    "laravel-pint.runInLaravelSail": false,
    "laravel-pint.fallbackToGlobalBin": false,
    "laravel-pint.executablePath": ".vscode/pint",
}